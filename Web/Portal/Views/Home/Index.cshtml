@model Cidade

@{
    ViewData["Title"] = "Home";
}

<div class="cidade">
    @for (int i = 0; i < Model.Linhas; i++)
    {
        <div class="linha">
            @for (int j = 0; j < Model.Colunas; j++)
            {
                <div class="celula @Model.Tipo" data-linha="@i" data-coluna="@j"></div>
            }
        </div>
    }
    <div>
        <button class="adicionarRuas">Adicionar Todas as Ruas</button>
        <button class="adicionarRua">Adicionar Rua</button>
        <button class="adicionarCasa">Adicionar Casa</button>
        <button class="resetarCidade">resetar Cidade</button>
    </div>
</div>